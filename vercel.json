{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    {
      "src": "^/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "^/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "^/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "^(?!/(css|js|images|build|favicon.ico)).*$",
      "dest": "/api/index.php"
    }
  ],
  "outputDirectory": "public",
  "buildCommand": "npm run prod",
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
